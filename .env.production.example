# Supabase (Get from: https://app.supabase.com/project/YOUR_PROJECT/settings/api)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Stripe (Get from: https://dashboard.stripe.com/apikeys)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_YOUR_KEY
STRIPE_SECRET_KEY=sk_live_YOUR_KEY
STRIPE_WEBHOOK_SECRET=whsec_YOUR_WEBHOOK_SECRET

# Google OAuth (Get from: https://console.cloud.google.com/apis/credentials)
GOOGLE_CLIENT_ID=YOUR_CLIENT_ID.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=YOUR_CLIENT_SECRET
NEXT_PUBLIC_APP_URL=https://postoko.com

# OpenAI (Get from: https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-YOUR_API_KEY

# Replicate (Get from: https://replicate.com/account/api-tokens)
REPLICATE_API_TOKEN=r8_YOUR_TOKEN

# Redis (For production, use Redis Cloud or similar)
REDIS_URL=redis://default:YOUR_PASSWORD@YOUR_HOST:6379

# Social Platforms (Configure after initial deployment)
INSTAGRAM_APP_ID=YOUR_APP_ID
INSTAGRAM_APP_SECRET=YOUR_APP_SECRET
TWITTER_CLIENT_ID=YOUR_CLIENT_ID
TWITTER_CLIENT_SECRET=YOUR_CLIENT_SECRET
PINTEREST_APP_ID=YOUR_APP_ID
PINTEREST_APP_SECRET=YOUR_APP_SECRET
LINKEDIN_CLIENT_ID=YOUR_CLIENT_ID
LINKEDIN_CLIENT_SECRET=YOUR_CLIENT_SECRET
TIKTOK_CLIENT_KEY=YOUR_CLIENT_KEY
TIKTOK_CLIENT_SECRET=YOUR_CLIENT_SECRET